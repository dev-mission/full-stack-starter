    "http-status-codes": "^2.2.0",
    "inflection": "^1.13.4",
    "luxon": "^3.2.1",
    "react": "^18.2.0",
    "react-bootstrap": "^2.7.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
