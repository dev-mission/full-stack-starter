{
  "name": "full-stack-starter-client",
  "private": true,
  "version": "5.0.2",
  "type": "module",
  "scripts": {
    "dev": "vite --host 0.0.0.0 --port 3000",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build --outDir dist/client",
    "build:server": "vite build --ssr src/entry-server.jsx --outDir dist/server",
    "lint": "eslint src --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "lint:changed": "eslint $(git diff --name-only --relative .; git ls-files -o --exclude-standard) --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "bootstrap": "^5.3.2",
    "classnames": "^2.3.2",
    "http-status-codes": "^2.3.0",
    "inflection": "^3.0.0",
    "luxon": "^3.4.4",
    "react": "^18.2.0",
    "react-bootstrap": "^2.9.1",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-helmet-async": "^2.0.1",
    "react-router-dom": "^6.20.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.38",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "eslint": "^8.54.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "sass": "^1.69.5",
    "vite": "^5.0.2"
  },
  "optionalDependencies": {
    "@swc/core-darwin-arm64": "^1.3.99",
    "@swc/core-darwin-x64": "^1.3.99",
    "@swc/core-linux-arm-gnueabihf": "^1.3.99",
    "@swc/core-linux-arm64-gnu": "^1.3.99",
    "@swc/core-linux-arm64-musl": "^1.3.99",
    "@swc/core-linux-x64-gnu": "^1.3.99",
    "@swc/core-linux-x64-musl": "^1.3.99",
    "@swc/core-win32-arm64-msvc": "^1.3.99",
    "@swc/core-win32-ia32-msvc": "^1.3.99",
    "@swc/core-win32-x64-msvc": "^1.3.99"
  }
}
